{
  "manifest_version": 3,
  "name": "OurschoolPages Chrome Extension",
  "description": "OurschoolPages Chrome Extension to help upload data to MemberPlanet",
  "version": "1.2",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webRequest"
  ],
  "chrome_url_overrides": {},
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "*.png",
        "*.gif"
      ],
      "matches": []
    }
  ]
}
